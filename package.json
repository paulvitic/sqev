{
  "name": "sqev",
  "version": "0.0.1",
  "description": "POC with event modelling, CQRS/ES and micro front-end",
  "private": false,
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "docker-compose -f docker-compose.yml up -d && PORT=3000 nodemon server/index.ts",
    "dev:debug": "nodemon --exec 'yarn compile && node -r ts-node/register --inspect-brk' server/index.ts",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet",
    "lint:fix": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest --testPathPattern=test\\.ts$ --passWithNoTests",
    "test:debug": "jest --inspect-brk --exit",
    "test:integration": "jest -w 1 --testPathPattern=intg\\\\.ts$ --passWithNoTests --detectOpenHandles",
    "test:functional": "jest --testPathPattern=func\\\\.ts$ --passWithNoTests",
    "db-migrate:test": "db-migrate up -e test -v",
    "db-migrate:prod": "db-migrate up -e prod -v",
    "local-environment": "docker-compose up -d",
    "deploy-service": "cd infrastructure/service && terraform taint module.ecs.aws_ecs_service.main && terraform apply -auto-approve"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.2.1",
    "axios": "^0.19.1",
    "fp-ts": "^2.9.1",
    "rxjs": "^6.6.3",
    "fp-ts-rxjs": "^0.6.11",
    "express": "^4.17.1",
    "express-list-middleware": "^1.0.1",
    "swagger-express-middleware": "^4.0.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "jsonwebtoken": "^8.5.1",
    "express-session": "^1.17.0",
    "connect-mongo": "^3.2.0",
    "mongodb": "^3.6.2",
    "db-migrate": "^0.11.11",
    "db-migrate-mongodb": "^1.5.0",
    "amqplib": "^0.6.0",
    "aws-sdk": "^2.814.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.15.16",
    "@types/swagger-express-middleware": "^1.0.10",
    "@types/amqplib": "^0.5.13",
    "@types/mongodb": "^3.5.27",
    "@types/db-migrate-base": "^0.0.8",
    "@types/jest": "^26.0.14",
    "@types/supertest": "^2.0.10",
    "typescript": "^4.1.3",
    "jest": "^26.6.3",
    "ts-lint": "^4.5.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "nodemon": "^2.0.6",
    "shelljs": "^0.8.4",
    "supertest": "^6.0.0",
    "nock": "^13.0.4"
  }
}
